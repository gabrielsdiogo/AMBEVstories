<name>sys_script_include_9f50949a1bb8751068eb0fe1f54bcbd0</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_script_include"&gt;&lt;sys_script_include action="INSERT_OR_UPDATE"&gt;&lt;access&gt;package_private&lt;/access&gt;&lt;active&gt;true&lt;/active&gt;&lt;api_name&gt;x_mdbda_tax_cnd.CNDUtils&lt;/api_name&gt;&lt;caller_access/&gt;&lt;client_callable&gt;true&lt;/client_callable&gt;&lt;description/&gt;&lt;name&gt;CNDUtils&lt;/name&gt;&lt;script&gt;&lt;![CDATA[var CNDUtils = Class.create();

CNDUtils.NEW = "1";
CNDUtils.WORK_IN_PROGRESS = "2";

CNDUtils.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

    getRecord: function(tableName, sysID) {

        var table_Name = tableName ? tableName : this.getParameter('sysparm_tableName');
        var sys_ID = sysID ? sysID : this.getParameter('sysparm_sysID');
        var record = new GlideRecord(table_Name);
        record.addQuery('sys_id', sys_ID);
        record.query();

        if (record.next()) {
            return record;
        }
    },

    tasksResolved: function(sysID) {
        var sys_id_cnd_issue = this.getParameter('sysparm_sysid_cnd_issue') || sysID;
        var tasks_concluidas = true;

        var taskGR = new GlideRecord('x_mdbda_tax_cnd_task');
        taskGR.addEncodedQuery('parent.sys_id=' + sys_id_cnd_issue);
        taskGR.query();

        while (taskGR.next()) {
            if (taskGR.getValue('state') != x_mdbda_tax_global.BaseCatalogUtils.CLOSED_COMPLETE) {
                tasks_concluidas = false;
            }
        }

        return tasks_concluidas;
    },

    getParentId: function(tableName, sysID) {
        var table_Name = tableName ? tableName : this.getParameter('sysparm_tableName');
        var sys_ID = sysID ? sysID : this.getParameter('sysparm_sysID');

        var rec = this.getRecord(table_Name, sys_ID);
        return rec.getValue('parent');

    },

    isChoice: function(tableName, fieldName, choiceValue) {
        var table_Name = tableName ? tableName : this.getParameter('sysparm_tableName');
        var field_Name = fieldName ? fieldName : this.getParameter('sysparm_fieldName');
        var choice_Value = choiceValue ? choiceValue : this.getParameter('sysparm_choiceValue');

        var choiceGR = new GlideRecord('sys_choice');
        choiceGR.addEncodedQuery('name=' + table_Name + '^element=' + field_Name + '^value=' + choice_Value);
        choiceGR.setLimit(1);
        choiceGR.query();

        if (choiceGR.next()) {
            return true;
        } else {
            return false;
        }
    },

    getFieldValue: function(tableName, fieldName, recordID) {
        var table_Name = tableName ? tableName : this.getParameter('sysparm_tableName');
        var field_Name = fieldName ? fieldName : this.getParameter('sysparm_fieldName');
        var record_ID = recordID ? recordID : this.getParameter('sysparm_recordID');

