    getDiasValidadeDataRenovacao: function(date) {
        obj = {
            "dias_vencimento": "",
            "data_renovacao": ""
        };

        var validade = date ? date : this.getParameter('sysparm_date');

        //Recebe a data no formato dd/MM/yyyy e converte para GlideDateTime
        var validade_convert = new GlideDate();
        validade_convert.setDisplayValue(validade);
        var validade_date_time = new GlideDateTime(validade_convert + ' 12:00:00');

        //Calcula os dias até o vencimento
        var today_date = new GlideDate();
        var today = new GlideDateTime(today_date.toString() + ' 12:00:00');

        var duration = GlideDateTime.subtract(today, validade_date_time);

        obj.dias_vencimento = duration.getDayPart();

        //Calcula a Data limite para solicitar renovação
        validade_date_time.addDaysLocalTime(-15);
        var data = validade_date_time.getDate().getByFormat('dd/MM/yyyy');
        var numero_dia_semana = validade_date_time.getDayOfWeekLocalTime();

        var dia_semana = {
            "1": "Segunda-feira",
            "2": "Terça-feira",
            "3": "Quarta-feira",
            "4": "Quinta-feira",
            "5": "Sexta-feira",
            "6": "Sábado",
            "7": "Domingo"
        };

        obj.data_renovacao = data + ' - ' + dia_semana[numero_dia_semana];

        return JSON.stringify(obj);
    },

    /**SNDOC
    	@name emailPendenciasDia
    	@description recebe o objeto current do disparo do evento da notificação, contendo o GlideRecord de um registro da CND que será utilizado como parâmetro para a construção da tabela, sendo utilizada a informação do Grupo.
    
    	@param {objeto} [current] - objeto current vindo da notificação
    
    	@returns {html} layout html para ser inserido no email script
    	*/
    emailPendenciasDia: function(current) {
        var info = this._buildObjectToNotifCND(current);
        var layoutHtml = "";
        var queryGridCND = "";
        var email_layout = gs.getProperty("x_mdbda_tax_cnd.layout.default.email.cnd"); //layout ambev
        var messageEmail = "";

        queryGridCND = gs.getProperty("x_mdbda_tax_cnd.layout.grid.email.cnd");
        messageEmail = "Gostaríamos de informar que novas pendências foram registradas na CND FEDERAL do GRUPO " + current.grupo.name + ". Abaixo você encontrará os detalhes.";

        var grGridEmailCND = new GlideRecord("sys_email_layout");
        grGridEmailCND.addQuery("name", queryGridCND);
        grGridEmailCND.query();

        var grLayoutCND = new GlideRecord("sys_email_layout");
        grLayoutCND.addQuery("name", email_layout); //layout ambev
        grLayoutCND.query();

        if (grLayoutCND.next() &amp;&amp; grGridEmailCND.next()) {
            layoutHtml = grLayoutCND.getValue("layout");
            var htmlTable = "";
            for (var i = 0; i &lt; info.length; i++) {
                gridLayout = grGridEmailCND.getValue("layout");
                htmlTable += this._buildHtmlToGrid(info[i], gridLayout);
            }

            var mensagem_final = '';
            var mensagem_final2 = '';
            if (info.length &gt; 0) {
                mensagem_final = 'Caso tenha alguma ação relevante a ser tomada em relação a estas pendências, por favor, clique no link ao lado das pendências.';
                mensagem_final2 = 'Agradecemos pela colaboração na resolução.';

                layoutHtml = layoutHtml.replaceAll("${mensagem_inicial}", messageEmail);
                layoutHtml = layoutHtml.replaceAll("${bodyMsg}", htmlTable);
                layoutHtml = layoutHtml.replaceAll("${mensagem_final1}", mensagem_final);
                layoutHtml = layoutHtml.replaceAll("${mensagem_final2}", mensagem_final2);
            } else {
                var instanceUrl = gs.getProperty('glide.servlet.uri') + gs.getProperty('x_mdbda_tax_cnd.modulo.cnd.ativas');
                mensagem_final = 'Para visualizar as pendências ativas existentes clicar no &lt;a href="' + instanceUrl + '"&gt;link&lt;\a&gt;';

                layoutHtml = layoutHtml.replaceAll("${mensagem_inicial}", "Nenhuma nova pendência foi registrada para o grupo.");
                layoutHtml = layoutHtml.replaceAll("${bodyMsg}", '');
                layoutHtml = layoutHtml.replaceAll("${mensagem_final1}", mensagem_final);
                layoutHtml = layoutHtml.replaceAll("${mensagem_final2}", '');
            }
        }

        return layoutHtml;
    },

    _buildHtmlToGrid: function(info, gridLayout) {
        gridLayout = gridLayout.replace("&lt;style&gt;", "&lt;style&gt;.MsoTableGrid td{border:1pt solid;padding: 0cm 5.4pt;font-size: 10pt;}");

        var html = "";
        for (var j = 0; j &lt; info.requisicoes.length; j++) {
            html += "&lt;tr&gt;&lt;td&gt;" + info.requisicoes[j].unidade + "&lt;/td&gt;";
            html += "&lt;td&gt;" + info.requisicoes[j].criticidade + "&lt;/td&gt;";
            html += '&lt;td&gt;' + info.requisicoes[j].tipo_pendencia + "&lt;/td&gt;";
            html += "&lt;td&gt;" + info.requisicoes[j].tipo_debito + "&lt;/td&gt;";
            html += '&lt;td&gt;' + info.requisicoes[j].origem + "&lt;/td&gt;";
            html += '&lt;td&gt;' + info.requisicoes[j].data_criacao + "&lt;/td&gt;";
            html += "&lt;td&gt;&lt;a href=" + info.requisicoes[j].link + "&gt;" + info.requisicoes[j].number + "&lt;/a&gt;&lt;/td&gt;&lt;/tr&gt;";
        }

        gridLayout = gridLayout.replace("&lt;/tbody&gt;", html + "&lt;/tbody&gt;&lt;br&gt;");

        return gridLayout;
    },

    _buildObjectToNotifCND: function(current) {
        var instanceUrl = gs.getProperty('glide.servlet.uri') + "nav_to.do?uri=";
        var arrObj = [];
        var queryCND = '';
        var today = new GlideDateTime();
        var firstdate = new GlideDateTime();
        if (today.getDayOfWeekLocalTime() == 1) {
            //Se é segunda, pega os registros de sexta (depois das 17h), sábado, domingo e segunda (até as 17h)
            firstdate.addDaysLocalTime(-3);
        } else { //Se não, começa pelo dia de ontem
            firstdate.addDaysLocalTime(-1);
        }

        queryCND = 'numberSTARTSWITHCNDP^grupo=' + current.grupo + "^sys_created_onBETWEENjavascript:gs.dateGenerate('" + firstdate.getDate() + "','17:00:00')@javascript:gs.dateGenerate('" + today.getDate() + "','16:59:59')^esfera=federal";

        var grCND = new GlideRecord('x_mdbda_tax_cnd_issue');
        grCND.addEncodedQuery(queryCND);
        grCND.orderBy('unidade.prioridade');
        grCND.orderBy('unidade.name');
        grCND.orderBy('tipo_pendencia');
        grCND.query();

        while (grCND.next()) {
            var obj = {};
            var req = {};
            var grupo = grCND.getDisplayValue('grupo');
            var link = instanceUrl + grCND.getLink(true);
            var data_criacao = grCND.getDisplayValue('data_da_criacao_do_debito');
            data_criacao = data_criacao.replace(' BRT', '');
            var index = this.searchNameReturningIndex(arrObj, grupo);
            if (index != -1) {
                req.unidade = grCND.getDisplayValue('unidade');
                req.criticidade = grCND.getDisplayValue('unidade.prioridade');
                req.tipo_pendencia = grCND.getDisplayValue('tipo_pendencia');
                req.tipo_debito = grCND.getDisplayValue('tipo_debito');
                req.origem = grCND.getDisplayValue('origem');
                req.data_criacao = data_criacao;
                req.number = grCND.getDisplayValue('number');
                req.link = link;
                arrObj[index].requisicoes.push(req);
            } else {
                obj.grupo = grupo;
                obj.requisicoes = [];
                req.unidade = grCND.getDisplayValue('unidade');
                req.criticidade = grCND.getDisplayValue('unidade.prioridade');
                req.tipo_pendencia = grCND.getDisplayValue('tipo_pendencia');
                req.tipo_debito = grCND.getDisplayValue('tipo_debito');
                req.origem = grCND.getDisplayValue('origem');
                req.data_criacao = data_criacao;
                req.number = grCND.getDisplayValue('number');
                req.link = link;
                obj.requisicoes.push(req);
                arrObj.push(obj);
            }
        }

        return arrObj;
    },

    searchNameReturningIndex: function(arrObj, grupo) {
        for (var i = 0; i &lt; arrObj.length; i++) {
            if (arrObj[i].grupo == grupo) {
                return i;
            }
        }
        return -1;
    },

